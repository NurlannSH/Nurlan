print('#1')
from datetime import datetime, timedelta

current_date = datetime.now()

new_date = current_date - timedelta(days=5)

print("Current Date:", current_date)
print("Date Five Days Ago:", new_date)

print('')
print('#2')
today = datetime.now()

yesterday = today - timedelta(days=1)
tomorrow = today + timedelta(days=1)

print("Yesterday:", yesterday)
print("Today:", today)
print("Tomorrow:", tomorrow)

print('')
print('#3')

current_datetime = datetime.now()

datetime_without_microseconds = current_datetime.replace(microsecond=0)

print("Current Datetime with Microseconds:", current_datetime)
print("Datetime without Microseconds:", datetime_without_microseconds)

print('')
print('#4')

date1 = datetime(2023, 6, 20, 14, 0, 0)
date2 = datetime(2023, 6, 25, 18, 30, 0)

difference_in_seconds = (date2 - date1).total_seconds()

print("Date 1:", date1)
print("Date 2:", date2)
print("Difference in Seconds:", difference_in_seconds)
